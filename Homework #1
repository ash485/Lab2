#include<stdio.h>

int main()
{
	
	int temp, celsius, count;
	count=0;
	temp=0;
	
	printf("\nFahrenheit		Celsius\n -----------------------------\n");
	while (count<31)
	{
    ++count;
    celsius=(temp-32)*5/9;
    
    printf("%d			 %d \n", temp, celsius);
    temp= 10+temp;
    }
    return(0);
}

